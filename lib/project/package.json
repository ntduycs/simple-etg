{
  "name": "generated-exporess-app",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "nodemon",
    "build": "webpack",
    "serve": "node dist/server.bundle.js --env=production",
    "prettier:check": "prettier --check 'src/**/*.ts'",
    "prettier:lint": "prettier --write 'src/**/*.ts'",
    "eslint:check": "eslint . --ext .ts --format table",
    "eslint:lint": "eslint . --ext .ts --quiet --fix",
    "lint:check": "npm-run-all prettier:check eslint:check",
    "lint": "npm-run-all prettier:lint eslint:lint"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@constants": "dist/constants",
    "@controllers": "dist/controllers",
    "@de": "dist/de",
    "@entities": "dist/entities",
    "@repositories": "dist/repositories",
    "@services": "dist/services",
    "@utils": "dist/utils"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "command-line-args": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.6",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@types/command-line-args": "^5.0.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.12",
    "@types/fs-extra": "^9.0.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fs-extra": "^10.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0"
  }
}
